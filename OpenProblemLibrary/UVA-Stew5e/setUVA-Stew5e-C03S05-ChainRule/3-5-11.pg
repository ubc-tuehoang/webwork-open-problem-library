DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,8,1);
$p = random(2,8,2);
$q = random(3,9,2);

$funct = "$p/$q*($a*t-$b/t)**($p/$q-1)*($a+$b/(t**2))";

Context("Fraction");
$pq=Compute("$p/$q");
Context()->texStrings;
BEGIN_TEXT
If \( f(t) = \displaystyle \left($a t - \frac{$b}{t} \right)^{$pq} \), find \( f'( t ) \).

$BR $BR \{ans_rule(50) \}
END_TEXT
Context()->normalStrings;
@ans = (fun_cmp("$funct", limits=>[10,11], vars=>"t"));
ANS(@ans);

Context("Fraction");
$pq1=Compute("$pq-1");
Context()->texStrings;
BEGIN_SOLUTION
$PAR SOLUTION $PAR 
\[ f'(t)= $pq  \left($a t - \frac{$b}{t} \right)^{$pq1} \frac{d}{dt}
\left($a t - \frac{$b}{t} \right) 
= $pq  \left($a t - \frac{$b}{t} \right)^{$pq1} 
\left($a  + \frac{$b}{t^2} \right) 
\]
END_SOLUTION

ENDDOCUMENT();        # This should be the last executable line in the problem.
