$f[3]=random(1,3,1);

$g[1]=random(1,3,1);
$g[2]=random(1,3,1);
$g[3]=random(1,3,1);

$fp[1]=random(1,3,1);
$fp[2]=random(1,3,1);
$fp[3]=random(1,3,1);

$gp[1]=random(1,3,1);
$gp[2]=random(1,3,1);
$gp[3]=random(1,3,1);

$p1=random(1,3,1);
$p2=random(1,3,1);


$y0=random(-9,9,1);
$y1=random(-9,9,1);
$y2=random(-9,9,1);
$y3=random(-9,9,1);
$y4=random(-9,9,1);
$y5=random(-9,9,1);
$y6=random(-9,9,1);
$a0 =random(-19,29,1);
