#####################################################################
DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
#####################################################################

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

#####################################################################

Context("Numeric");

$a_numb = random(2,9,1);
$a_sign = random(-1,1,2);
$a      = $a_numb * $a_sign;

$b_numb = random(2,9,1);
$b_sign = random(0,0,2);
$b      = $b_numb * $b_sign;

$c_numb = random(2,9,1);
$c_sign = random(-1,1,2);
$c      = $c_numb * $c_sign;

$d_numb = random(2,9,1);
$d_sign = random(-1,1,2);
$d      = $d_numb * $d_sign;

$e_numb = random(2,9,1);
$e_sign = random(-1,1,2);
$e1      = $e_numb * $e_sign;

$m = random(3,8,1);
$n = random(9,15,1);

#####################################################################
Context()->texStrings;
BEGIN_TEXT

Let \[ f(x) = ($a x^2 + $c)^{$m} ($d x^2 + $e1)^{$n}  \]
$PAR
\( f'( x ) = \) \{ans_rule(100) \}

END_TEXT
Context()->normalStrings;

#####################################################################

$ans = Compute("($m*($a*x**2 + $c)**($m-1))*(2*$a*x)*($d*x**2 + $e1)**($n) +
	($n*($d*x**2 + $e1)**($n-1))*(2*$d*x)*($a*x**2+$c)**($m)");
ANS($ans -> cmp);

ENDDOCUMENT();        # This should be the last executable line in the problem.
