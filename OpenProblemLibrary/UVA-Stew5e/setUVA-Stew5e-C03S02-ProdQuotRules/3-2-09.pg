DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

Context('Numeric');

$a = random(2, 13, 1);
$b = random(2,7,1);
$r1 = random(1,12,1);
$r2 = random(-12,-1,1);

BEGIN_TEXT
Let \( f(x) = $a x^3 (x^2 - $b) \).
Evaluate \( f'(x) \) at the following points:
$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
(B) \( f'($r2) \) = \{ans_rule(40) \}
END_TEXT

$ansA = Compute("3*$a*(($r1)**2)*(($r1)**2 - $b) + $a*(($r1)**3)*2*($r1)");
ANS($ansA->cmp);

$ansB = Compute("3*$a*(($r2)**2)*(($r2)**2 - $b) + $a*(($r2)**3)*2*($r2)");
ANS($ansB->cmp);

ENDDOCUMENT();
