DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;
Context("Numeric");

$a =  random(1,5,1);
$ans = Real("ln($a + ($a**2 - 1)**(1/2))");

Context()->functions->disable("All");

BEGIN_TEXT
Find the numerical value of the expression: 
$BR $BR
\(
\cosh^{-1} ($a)
=
\)
\{ans_rule(30) \}
END_TEXT

ANS($ans->cmp() );

