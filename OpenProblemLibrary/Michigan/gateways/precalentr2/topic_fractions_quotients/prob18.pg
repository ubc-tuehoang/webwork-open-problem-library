## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(t\over k^2 + kt\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({ {5t\over k} \over  5t
+ 5k }  \)?',
	    $ans);
$gwprob->extra("\(25 t(t + k)\over k\)",
	"\(t\over k(t + 5k)\)",
	"\(1\over 5k^2\)",
	"\(1\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
