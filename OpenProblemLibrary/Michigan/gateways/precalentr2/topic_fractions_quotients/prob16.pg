## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(a + 3\over 3a\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( \displaystyle{ {a\over
x} + {3\over x} \over  {3a\over x}}\)?',
	    $ans);
$gwprob->extra("\(a + 3\over 3ax\)",
	"\(3a(a + 3)\over 2x^2\)",
	"\(3a(a + 3)\over x^2\)",
	"\(a + 3\over 6a\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
