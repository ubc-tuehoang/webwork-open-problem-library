## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(9 + u^2\over 1 + 3u\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({u + {9\over u}\over 3 +
{1\over u}}\)?',
	    $ans);
$gwprob->extra("\(u^2 + {u\over 3} + {3\over u} + 9\)",
	"\((u^2+9)(3u+1)\over u^2\)",
	"\(u + 3\)",
	"\(u^2 + 9\over 3\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
