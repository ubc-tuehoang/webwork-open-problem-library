## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(1\over 8z\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({ {1\over z} + {1\over 8}
\over  z + 8 }  \)?',
	    $ans);
$gwprob->extra("\(1\)",
	"\(1\over (z + 8)^2\)",
	"\(z + 8\)",
	"\((z+8)^2\over 8z\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
