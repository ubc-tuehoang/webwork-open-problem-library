## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(8c + 5w\over w^2\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({ {8\over w} + {5\over c}
\over  {w\over c} }\)?',
	    $ans);
$gwprob->extra("\(8w + 5c\over w^2\)",
	"\({8\over c} + 5w\)",
	"\(8 c + 5 w^2\over w(w+c)\)",
	"\(13\over w\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
