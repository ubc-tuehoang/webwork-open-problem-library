## DESCRIPTION
## Michigan precalculus gateway problem: topic quotients of fractions
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(9b + 5u\over b\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( { {b\over u} + {5\over
9} } \over  { b\over 9u }  \)?',
	    $ans);
$gwprob->extra("\(b^2 + 5 b\over (9u)^2\)",
	"\(9b^2 + 5 b u\over (9u)^2\)",
	"\(b + 5\over b\)",
	"\(9b + 5\over bu\)");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
