## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = " None of these expressions are equivalent ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (w^{8}\, K)^{5/4} \)?',
	    $ans);
$gwprob->extra(" \( w^{5/2}\, K^{5/4} \) ",
	" \( w^{37/4} \, K^{9/4} \)  ",
	" \( w^{32/5} \, K^{4/5} \)  ",
	" \( (w\, K)^{10} \)  ");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
