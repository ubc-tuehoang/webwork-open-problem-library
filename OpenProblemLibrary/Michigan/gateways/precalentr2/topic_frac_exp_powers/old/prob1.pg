## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(x^{1/3} y^{2/3}\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((x y^2)^{1/3}\)?',
	    $ans);
$gwprob->extra("\((x y)^{2/3}\)",
	"\(x^{1/3} y\)",
	"\(x^{1/3} y^{7/3}\)",
	"None of these expressions are equivalent");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
