## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = "\(c^3 d^2\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((c^{3/2} d)^2\)?',
	    $ans);
$gwprob->extra("\(c^6 d^2\)",
	"\((c d)^{3}\)",
	"\(c^{7/2} d^2\)",
	"None of these expressions are equivalent");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
