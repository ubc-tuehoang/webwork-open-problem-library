## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = " \( K^{2}\, Z^{5/3} \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (K^{6/5}\, Z)^{5/3} \)?
',
	    $ans);
$gwprob->extra(" \( K^{43/15} \, Z^{8/3} \)  ",
	" \( K^{18/25} \, Z^{3/5} \)  ",
	" \( K^{6/5}\, Z^{5/3} \)  ",
	" None of these expressions are equivalent ");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
