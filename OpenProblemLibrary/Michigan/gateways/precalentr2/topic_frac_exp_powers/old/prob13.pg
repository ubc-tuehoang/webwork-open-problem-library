## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = " \( d^{1/2}\, t^{2/3} \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (d^{6/8}\, t)^{2/3} \)?
',
	    $ans);
$gwprob->extra(" \( d^{17/12} \, t^{5/3} \)  ",
	" \( d^{9/8} \, t^{3/2} \)  ",
	" \( d^{1/2}\, t^{5/3} \)  ",
	" None of these expressions are equivalent ");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
