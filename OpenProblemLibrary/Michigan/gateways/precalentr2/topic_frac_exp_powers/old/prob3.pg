## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = " \( a^{9/8}\, h \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (a\, h^{8/9})^{9/8} \)?
',
	    $ans);
$gwprob->extra(" \( a^{17/8} \, h^{145/72} \)  ",
	" \( a^{8/9} \, h^{64/81} \)  ",
	" None of these expressions are equivalent ",
	" \( a^{8/9}\, h \)  ");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
