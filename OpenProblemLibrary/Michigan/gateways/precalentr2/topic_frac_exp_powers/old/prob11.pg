## DESCRIPTION
## Michigan precalculus gateway problem: topic powers of fractional exponents
## ENDDESCRIPTION

DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

$ans = " \( e^{6}\, s^{9/4} \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (e^{8/3}\, s)^{9/4} \)?
',
	    $ans);
$gwprob->extra(" \( e^{8/3} \, s^{9/4} \)  ",
	" \( e^{59/12} \, s^{13/4} \)  ",
	" \( e^{59/12}\, s^{9/4} \)  ",
	" None of these expressions are equivalent ");



Context()->texStrings;
BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT
Context()->normalStrings;

ANS(radio_cmp( $gwprob->correct_ans() ));

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

[The solution needs to be filled in for this problem.]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
