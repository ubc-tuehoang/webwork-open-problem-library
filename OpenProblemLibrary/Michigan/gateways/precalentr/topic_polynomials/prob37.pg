TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \((w-5)(w+1)\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((w-2)^2-9\)?',
	    $ans);
$gwprob->extra(" \(((w-2) - 3)^2\) ",
	" \((w-1)^2\) ",
	" \((w-2)(w-3)\) ",
	" \(-(w+5)(w+1)\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

