## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Antiderivatives (without trigonometric functions))
## Institution(UMich)
## Author(Gavin LaRose)
## MLT(antideriv_poly_find_AN_antideriv)
## Level(2)
## MO(1)
## TitleText1('Calculus')
## TitleText2('Calculus')
## TitleText3('Calculus')
## AuthorText1('Hughes-Hallett')
## AuthorText2('Hughes-Hallett')
## AuthorText3('Hughes-Hallett')
## EditionText1('4')
## EditionText2('5')
## EditionText3('6')
## Section1('6.2')
## Section2(7.Review)
## Section3(7.Review)
## Problem1('19')
## Problem2(1)
## Problem3(1)

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

Context("Numeric");
Context()->variables->add(t=>"Real");
$showPartialCorrectAnswers = 1;

$a = random(1,9,1);
$b = random(2,5,1);
$b1 = $b+1;
$func = Compute("(t + $a)^($b)");
$afunc = Compute("(1/$b1)*(t + $a)^($b1)");
 
TEXT(beginproblem());
Context()->texStrings;
BEGIN_TEXT

Find an antiderivative of
\( q(t) = $func \)

$PAR
\( Q(t) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS($afunc->cmp( upToConstant=>1 ) );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ Q(t) = {1\over $b1}(t + $a)^{$b1} \]

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
