DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
Context("Numeric");

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of \( f(x)=\sin($a x) \)

$PAR
\( f'(x) = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "$a*cos($a*x)" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$BR SOLUTION $BR

\[ f'(x) = \cos($a x)\cdot $a = $a\cos($a x) \]

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
