DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
Context("Numeric");
Context()->variables->add( q=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

@const = ( '\pi', '(\sqrt{2})', 'e', '(\ln(3))', '4.15', '(\sin(1))' );
@conste = ( 'pi', 'sqrt(2)', 'e', 'ln(3)', '4.15', 'sin(1)' );
$i = random(0,scalar(@const)-1,1);
$c = $const[ $i ];
$ce = $conste[ $i ];

Context()->texStrings;
BEGIN_TEXT

Find the derivative of 
\( z = q\cdot $c^{\cos q} \)

$PAR
\( {dz\over dq} = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "($ce)^(cos(q)) - q*sin(q)*ln($ce)*($ce)^(cos(q))" )->cmp() );

$lnc = ( $c eq 'e' ) ? '' : "\ln($c)";
$lnc =~ s/~~(~~(/~~(/;
$lnc =~ s/~~)~~)/~~)/;

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ {dz\over dq} = $c^{\cos(q)} - q \sin(q) $lnc $c^{\cos(q)} \]

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
