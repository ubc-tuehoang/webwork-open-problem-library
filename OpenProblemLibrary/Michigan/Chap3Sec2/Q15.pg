DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");
Context()->variables->add( z=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of 
\( h(z) = (\ln$a)^z \).

$BR $BR
\( h'(z) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS(Compute( "ln(ln($a))*ln($a)^z" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));

\[ h'(z) = \ln(\ln($a)) \cdot (\ln($a))^z \]
END_SOLUTION
Context()->normalStrings;

;
ENDDOCUMENT();
