DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context("Numeric");

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,15,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of 
\( z = (\ln $a) e^x \).

$BR $BR
\( {dz\over dx} = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "ln($a)*e^x" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
\[ {dz\over dx} = \ln($a) e^x \]
END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
