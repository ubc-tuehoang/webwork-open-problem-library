DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
Context("Numeric");
Context()->flags->set( limits=>[0.1,5] );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

@const = ('e', '\pi', '\sqrt 2', '\ln 3', '\sin 1.57');
$d = $const[ random(0, (@const-2), 1) ];
$a = random(2,9,1);
$b = random(2,9,1);
$c = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of the function \( y \), below.
$BR
\( y = $a x (\ln x + \ln $b) - $c x + $d \)

$BR$BR
\( {dy\over dx} = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "$a*(ln(x) + ln($b)) + $a - $c" )->cmp() );

if ( $a == $c ) {
   $amc = '';
} elsif ( $a > $c ) {
   $amc = " + " . ($a-$c);
} else {
   $amc = " - " . ($c-$a);
}

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));

\( y' = $a (\ln(x) + \ln($b)) + ($a x)\left(\frac1x\right) - $c \), or
\( y' = $a (\ln(x) + \ln($b)) $amc \).

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
