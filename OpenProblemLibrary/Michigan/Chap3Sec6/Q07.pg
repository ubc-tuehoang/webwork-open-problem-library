DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
Context("Numeric");

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1,21,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of the function \( f(x) \), below.
$BR
 \( f(x) = \ln(e^x + $a) \)

$BR$BR
\( f'(x) = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "(e^(x)/(e^(x) + $a))" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));

\( f'(x) = \frac{e^x}{e^x + $a} \)

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
