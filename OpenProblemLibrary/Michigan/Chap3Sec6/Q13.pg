DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
Context("Numeric");

Context()->variables->add( w=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$whichtrig = random(0,1,1);
$a = random(1,15,1);
if ( $whichtrig ) {
    $g = '\sin';
    $ans = "cos(w - $a)/sin(w - $a)";
    $soln ="\frac{\cos(w - $a)}{\sin(w - $a)}";
} else {
    $g = '\cos';
    $ans = "-1*tan(w - $a)";
    $soln = "\frac{-\sin(w-$a)}{\cos(w-$a)} = -\tan(w - $a)";
}

Context()->texStrings;
BEGIN_TEXT

Find the derivative of the function \( f(w) \), below.
$BR 
 \( f(w) = \ln($g(w-$a)) \)

$BR
\( f'(w) = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "$ans" )->cmp);

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$BR SOLUTION $BR

\(\displaystyle f'(w) = $soln \)

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
