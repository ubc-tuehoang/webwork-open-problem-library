DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

Context("Numeric");
Context()->variables->add( t=>'Real' );
$showPartialCorrectAnswers = 1;

$whichF = random(0,3);
if ( $whichF == 0 ) {
    $f = Compute("cosh(t)");
    $g = Compute("sinh(t)");
    $fg = Compute("cosh(sinh(t))");
} elsif ( $whichF == 1 ) {
    $f = Compute("sinh(t)");
    $g = Compute("cosh(t)");
    $fg = Compute("sinh(cosh(t))");
} elsif ( $whichF == 2 ) {
    $f = Compute("cosh(t)");
    $g = Compute("cosh(t)");
    $fg = Compute("cosh(cosh(t))");
} else {
    $f = Compute("sinh(t)");
    $g = Compute("sinh(t)");
    $fg = Compute("sinh(sinh(t))");
}
$dfg = $fg->D('t');

TEXT(beginproblem());
Context()->texStrings;
BEGIN_TEXT

Find the derivative of
\( f(t) = $fg \)

$PAR
\( f'(t) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS($dfg->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

Using the chain rule,
\[
\frac{d}{dt}\left( $fg \right) =
$dfg. \]

END_SOLUTION
Context()->normalStrings;
Context()->normalStrings;


;
ENDDOCUMENT();
