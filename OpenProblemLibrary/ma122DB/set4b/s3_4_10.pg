DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,5,1);
$b1 = random(1,3,1);
$x1 = random(1,5,1);
$funct1 = "($a1*cos(x)*$b1 +$a1)/($b1+cos(x))^2";

BEGIN_TEXT
If 
\[ f(x) = \frac { $a1 \sin x }{ $b1 + \cos x } \]
find \( f'( x ) \).
$BR$BR Answer:  \{ans_rule(50) \}
END_TEXT

Context("Numeric");

ANS(Compute($funct1)->cmp);

