DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
Context('Numeric');

$an = random(2,5,1);
$as = random(-1,1,2);
$a  = $an * $as;
$b  = random(2,8,1);
$x  = random(2,5,1);

$ans = Formula("$a/x")->reduce;

BEGIN_TEXT

Let \[ f(x) = $a \ln($b x) \]
$BR
\( f'( x ) = \) \{ans_rule(40) \}

END_TEXT

ANS($ans->cmp());


ENDDOCUMENT();        # This should be the last executable line in the problem.
