## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Definite integrals (with trigonometric functions))
## Date(6/3/2002)
## Level(2)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('6')
## Section1('5.3')
## Problem1('32')
## KEYWORDS('integrals', 'definite')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,9,1);
$a = random(.1,.8,.1);
$b = random($a+0.1,1.5,.1);

BEGIN_TEXT
Evaluate the definite integral
\[ \int_{ $a } ^ { $b } $a1 \sec^2(x) \, dx \]
Answer: \{ans_rule(40) \}
END_TEXT

ANS(num_cmp( "$a1*(tan($b)-(tan($a)))" ));

BEGIN_SOLUTION
$PAR SOLUTION $PAR 
\[  \begin{array}{rcl} \displaystyle \int_{ $a } ^ { $b } $a1 \sec^2(x) \, dx 
&=& \displaystyle $a1  \int_{ $a } ^ { $b }  \sec^2(x) \, dx
\\ &=&  $a1  \tan (x) \Big]_{$a}^{$b}
\\ &=&$a1 (\tan($b)-\tan($a)) 
\end{array} \]
END_SOLUTION
ENDDOCUMENT();        # This should be the last executable line in the problem.
