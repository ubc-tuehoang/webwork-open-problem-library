DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$c[0] = new_checkbox_multiple_choice();
$c[1] = new_checkbox_multiple_choice();
$c[2]  = new_checkbox_multiple_choice();
$c[3] = new_checkbox_multiple_choice();

$n = random(0,5,1);
($A0,$A1,$A2) = ($n, $n+2, $n+4);
($B0,$B1) = ($n+1, $n+2); 

#$tf = new_pop_up_select_list();   #For popups!

#Question and matching correct answers

$c[0] -> qa (
"(a) \( ($A0, $B0) \)",
"\( A \times B \)"
);

#Some incorrect answers

$c[0] -> extra(
"\( B \times A \)",
"\( A \times A \)",
"\( B \times B \)"
);

$c[1] -> qa(
"(b) \( ($A1,$A0) \)",
"\( B \times A \)",
"\( A \times A \)"
);

$c[1] -> extra(
"\( B \times B \)",
"\( A \times B \)"
);

$c[2] -> qa(
"(c) \( ($A0,$A0) \)",
"\( A \times A \)"
);

$c[2] -> extra(
"\( A \times B \)",
"\( B \times A \)",
"\( B \times B \)"
);

$c[3] -> qa(
"(d) \( ($A1,$A1) \)",
"\( A \times A \)",
"\( A \times B \)",
"\( B \times A \)",
"\( B \times B \)"
);

# permute 4 questions
@q = (0..3);
for ($i=0;$i<3;$i++){
  $j = random($i,3,1);
  ($q[$i],$q[$j])=($q[$j],$q[$i]);
}


BEGIN_TEXT
\( A = \lbrace $A0, $A1, $A2 \rbrace, B = \lbrace $B0,$B1 \rbrace \)
$PAR
Check ALL of the following Cartesian products to which the following
elements belong: $BR
\{ $c[$q[0]] -> print_q \}
$PAR
\{ $c[$q[0]] -> print_a \}
$PAR
\{ $c[$q[1]] -> print_q \}
$PAR
\{ $c[$q[1]] -> print_a \}
$PAR
\{ $c[$q[2]] -> print_q \}
$PAR
\{ $c[$q[2]] -> print_a \}
$PAR
\{ $c[$q[3]] -> print_q \}
$PAR
\{ $c[$q[3]] -> print_a \}
$PAR
END_TEXT

ANS(checkbox_cmp( $c[$q[0]] -> correct_ans ) );
ANS(checkbox_cmp( $c[$q[1]] -> correct_ans ) );
ANS(checkbox_cmp( $c[$q[2]] -> correct_ans ) );
ANS(checkbox_cmp( $c[$q[3]] -> correct_ans ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
