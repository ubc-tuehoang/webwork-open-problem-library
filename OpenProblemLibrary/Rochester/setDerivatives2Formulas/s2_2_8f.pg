TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are(t => "Real");

$a1 = random(2,7,1);
$b1 = random(2,8,1);
$c1 = random(2,7,1);
$d1 = random(2,6,1);
$n1 = random(3,7,1);

$funct1 = Compute("(sqrt($a1))*(-$n1)*t^(-$n1-1)");
BEGIN_TEXT
If 
\[ f(t) =  \frac { \sqrt {$a1 }} {t ^{ $n1 } } \]
find \( f'( t ) \).
$BR $BR \( f'( t ) \) = \{ans_rule(48) \}
END_TEXT

$ans = $funct1;
ANS($funct1->cmp);







