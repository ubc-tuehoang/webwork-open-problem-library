## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Derivatives involving multiple rules (all rules))
## Institution(OSU)
## Level(3)
## MO(1)
## KEYWORDS('calculus','derivative','product rule','quotient rule','chain rule','trigonometric')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context('Numeric');

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(3,7,1);
$c = random(2,5,1);
$d = random(3,8,1);
$m = random(2,5,1);
$n = random(3,6,1);

$fcn1 = Formula("x^$m*sin(x^$n)")->reduce;
$fcn2 = Formula("sqrt($a x^2 - $b)")->reduce;
$fcn3 = Formula("($c x + $b)/($a x^2 + $d)")->reduce;

$ans1 = Compute("$m*x^($m-1)*sin(x^$n)+$n*x^($m+$n-1)*cos(x^$n)");
$ans3 = Compute("($c*($a*x^2+$d)-2*$a*x*($c*x+$b))/($a*x^2+$d)^2");

Context()->variables->set(x=>{limits=>[$b+1,$b+2]});
$ans2 = Compute("$a*x/sqrt($a*x^2-$b)");

Context()->texStrings;

BEGIN_TEXT
Find the derivative of
\[f(x) = $fcn1 \]
\(f'(x) = \) \{ans_rule(60)\}
$BR$BR
Find the derivative of
\[g(x) = $fcn2 \]
\(g'(x) = \) \{ans_rule(60)\}
$BR$BR
Find the derivative of
\[h(x)= $fcn3 \]
\(h'(x) = \) \{ans_rule(60)\}
$BR
END_TEXT

Context()->normalStrings;

ANS($ans1 -> cmp);
ANS($ans2 -> cmp);
ANS($ans3 -> cmp);

ENDDOCUMENT();        # This should be the last executable line in the problem.

