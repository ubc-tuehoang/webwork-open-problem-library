DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem);
$showPartialCorrectAnswers = 1;

$pi = arccos(-1);

$a_n = random(2,12,1);
$a_s = random(-1,1,2);
$a   = $a_n * $a_s;

$b_n = random(2,12,1);
$b_s = random(-1,1,2);
$b   = $b_n * $b_s;

$funct1 ="$a*sec(x)**2 + $b*sec(x)*tan(x)";

BEGIN_TEXT
Let \[ f(x) = \frac { $a \sin(x) + $b }{ \cos(x) }. \]
$BR $BR
Find \( f'( x ) = \) \{ans_rule(45) \}
END_TEXT

Context("Numeric");

$ans = Compute($funct1);
ANS($ans->cmp);


