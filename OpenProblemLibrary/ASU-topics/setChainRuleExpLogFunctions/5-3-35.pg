DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context("Numeric");
Context()->variables->are(t=>'Real');

$a = random(3,6,1);
$b = random(3,12,1);
$c = random(3,12,1);
$ans = Formula("(2*$a*t + $b)/($a*t**2 + $b*t + $c)");

BEGIN_TEXT
Evaluate
$BR
$BR
\(\displaystyle{\frac{d}{dt}\ln($a t^2 + $b t + $c)}\) = \{ans_rule(20) \}
$BR
END_TEXT

ANS($ans->cmp());

ENDDOCUMENT();
