DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

Context("Numeric");

$a = random(3,6,1);
$b = random(3,12,1);
$xp = random(-2,2,1);

$ans = Compute("(2 $a $xp)/exp($xp) - ($a ($xp)^2 + $b)/exp($xp)");

BEGIN_TEXT
Evaluate
\(\displaystyle{\frac{d}{dx}($a x^2 + $b)e^{-x}}\) at \(x=$xp\).
$BR
$BR
\(\displaystyle \left.\frac{d}{dx}($a x^2 + $b)e^{-x} \right|_{x=$xp}\) = \{ans_rule(20) \}
END_TEXT

ANS($ans -> cmp);

ENDDOCUMENT();
