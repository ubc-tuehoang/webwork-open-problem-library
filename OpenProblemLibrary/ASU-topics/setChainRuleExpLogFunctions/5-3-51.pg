DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context("Numeric");

$a = random(3,5,1);
$b = random(3,12,1);
$ans = Compute("(2*(ln(". ($b-1) ."))**(-1+1/$a))/(". ($a - $a*$b) .")");

BEGIN_TEXT
Evaluate 
\(\displaystyle{\frac{d}{dx}\sqrt[$a]{\ln($b - x^2)}}\) at \(x=1\).
$BR $BR
Answer: \{ans_rule(20) \}
END_TEXT

ANS($ans->cmp());

ENDDOCUMENT();
