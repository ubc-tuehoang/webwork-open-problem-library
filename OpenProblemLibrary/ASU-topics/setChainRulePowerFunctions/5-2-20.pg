DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context("Numeric");

$a = random(2,20,1);
$b = random(2,20,1);
$xp = random(2,20,1);

$ans = Compute("($xp-$a)**$b*exp($xp) + exp($xp)*$b*($xp-$a)**($b-1)");

BEGIN_TEXT
Suppose that \( f(x) = (x-$a)^{$b} e^{x} \).  Find \(f'($xp)\).
$BR $BR
\(f'($xp)\) = \{ans_rule(30) \}
END_TEXT


ANS($ans -> cmp);

ENDDOCUMENT();


