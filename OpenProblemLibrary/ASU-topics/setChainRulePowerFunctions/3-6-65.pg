DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

Context('Numeric');

$a = random(1,4,1);
$b = random(3,5,1);
$c = non_zero_random(-3,3,1);
while ($c**2 == 1)
{
$c = non_zero_random(-3,3,1);
}
$p1 = random(1,2,1);
$p2 = random(-2,-1,1);

$fcn = Formula("((x^3 - $a)^($b))/($c x^3)") -> reduce;
$ans1 = Compute("(($c*($p1)**3)*($b*(($p1)**3 -$a)**($b-1))*(3*($p1)**2) -
(((($p1)**3 - $a)**($b))*(3*$c*($p1)**2)))/($c*($p1)**3)**2");
$ans2 = Compute("(($c*($p2)**3)*($b*(($p2)**3 -$a)**($b-1))*(3*($p2)**2) -
(((($p2)**3 - $a)**($b))*(3*$c*($p2)**2)))/($c*($p2)**3)**2");

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

Context()-> texStrings;

BEGIN_TEXT
Suppose that
\[  f(x) = $fcn.  \]
Find \(f'(x)\), and then evaluate \(f'\) at \(x = $p1\) and
\(x = $p2\).
$BR$BR
\(f'($p1)\) = \{ans_rule(30)\}

$BR$BR
\(f'($p2)\) = \{ans_rule(30)\}
END_TEXT

Context() -> normalStrings;

ANS($ans1 -> cmp);
ANS($ans2 -> cmp);

ENDDOCUMENT();        # This should be the last executable line in the problem.
