## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')
## MO(1)


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context('Numeric');

$a = random(2, 13, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$d = random(3,9,1);
$r1 = random(1,12,1);
$r2 = random(-12,-1,1);
while ($c*($r1) - $d == 0)
{
$r1 = random(1,12,1);
}
$r2 = random(-12,-1,1);
while ($c*($r2) - $d == 0)
{
$r2 = random(-12,-1,1);
}

BEGIN_TEXT
Let
\[ f(x) = \frac{$a x - $b}{$c x - $d}. \]
Evaluate \( f'(x) \) at the following points:
$BR $BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
$BR
END_TEXT

$ans = Compute("($a*($c*($r1) -$d) - $c*($a*($r1) - $b))/(($c*($r1) - $d)**2)");
ANS($ans->cmp);


BEGIN_TEXT
(B) \( f'($r2) \) = \{ans_rule(40) \}
END_TEXT

$ans2 = Compute("($a*($c*($r2) -$d) - $c*($a*($r2) - $b))/(($c*($r2) - $d)**2)");
ANS($ans2->cmp);


ENDDOCUMENT();
