## Level(3)
## MO(1)
## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

Context('Numeric');

$a = random(2, 13, 1);
$b = random(2,5,0.3);
$c = random(2,7,.5);
$d = non_zero_random(-9,9,1);
while ($d**2 == 1)
{
$d = random(-9,9,1);
}
$r1 = random(1,12,1);
$r2 = random(-12,-1,1);

BEGIN_TEXT
Let \( f(x) = ($a - $b x^3)($c x^2 ? {$d} x) \).
Evaluate \( f'(x) \) at the following points:
$BR
$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
$BR
(B) \( f'($r2) \) = \{ans_rule(40) \}
END_TEXT

$ans1 = Compute("(-3*$b*($r1)**2)*($c*(($r1)**2) + $d*$r1) + ($a - $b*($r1)**3)*(2*$c*($r1) + $d)");

$ans2 = Compute("(-3*$b*($r2)**2)*($c*(($r2)**2) + $d*$r2) + ($a - $b*($r2)**3)*(2*$c*($r2) + $d)");

ANS($ans1->cmp);
ANS($ans2->cmp);


ENDDOCUMENT();
