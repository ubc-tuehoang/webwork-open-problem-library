DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Numeric');

$a = random(1,5,1);

BEGIN_TEXT
Suppose that \( f(x) = x e^{x} - e^{x} \).  Find \(f'($a)\).

$BR $BR

\(f'($a)\) = \{ans_rule(30) \}
END_TEXT

$ans = Compute("$a*exp($a)");
ANS($ans->cmp);

ENDDOCUMENT();


