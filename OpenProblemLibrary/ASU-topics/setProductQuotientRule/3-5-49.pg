DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

Context('Numeric');

$a = random(2, 8, 1);
$b = random(3,5,1);
$c = random(2,11,1);
$r1 = random(1,3,1);
$r2 = random(4,6,1);

BEGIN_TEXT
Let
\[ f(x) = $a x^{1/$b}(x^3 - $c). \]
Evaluate the following specific values of \( f' \):
$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
(B) \( f'($r2) \) = \{ans_rule(40) \}
END_TEXT

$ansA =
Compute("($a/$b)*(($r1)**((1/$b)-1))*(($r1)**3-$c)+($a*($r1)**(1/$b))*(3*$r1**2)");

ANS($ansA->cmp);

$ansB = Compute("($a/$b)*(($r2)**((1/$b)-1))*(($r2)**3-$c)+($a*($r2)**(1/$b))*(3*$r2**2)");

ANS($ansB->cmp);

ENDDOCUMENT();
