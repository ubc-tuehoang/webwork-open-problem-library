DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(2, 13, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$d = random(3,9,1);
$r1 = random(1,12,1);
$r2 = random(-12,0,1);

BEGIN_TEXT
Let
\[ f(x) = \frac{$a x^2 - $b}{$c x^2 + $d}. \]
Evaluate \( f'(x) \) at the following points:
$BR$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
$BR
END_TEXT

$ans = Compute("(2*$a*(($r1))*($c*(($r1)**2) + $d) - 2*$c*($r1)*($a*(($r1)**2) - $b))/(($c*(($r1)**2) + $d)**2)");
ANS($ans->cmp);

BEGIN_TEXT
(B) \( f'($r2) \) = \{ans_rule(40) \}
$BR
END_TEXT

$ans2 = Compute("(2*$a*(($r2))*($c*(($r2)**2) + $d) - 2*$c*($r2)*($a*(($r2)**2) - $b))/(($c*(($r2)**2) + $d)**2)");
ANS($ans2->cmp);

ENDDOCUMENT();
