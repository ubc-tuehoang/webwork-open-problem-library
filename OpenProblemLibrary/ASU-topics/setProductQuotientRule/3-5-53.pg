## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')
## MO(1)


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context('Numeric');

$a = random(2, 6, 1);
$b = random(3,7,1);
$c = random(2,4,2);
$r1 = random(1,3,1);
$r2 = random(4,6,1);

BEGIN_TEXT
Let \( \displaystyle f(x) = \frac{$a x^3 - $b x^2}{\sqrt[3]{x^{$c}}} \).
Evaluate \( f'(x) \) at the following points:
$BR$BR
(a) \( f'($r1)\) = \{ans_rule(40) \}
$BR$BR
END_TEXT

$ans = Compute("((3*$a*$r1**2 - 2*$b*$r1)*($r1**($c/3))-($a*$r1**3 -
$b*$r1**2)*(($c/3)*$r1**(($c/3)-1)))/($r1**($c/3))**2");

ANS($ans->cmp);
#ANS(num_cmp($ans));

BEGIN_TEXT
(b) \( f'($r2) \) = \{ans_rule(40) \}
END_TEXT

$ans2 = Compute("((3*$a*$r2**2 - 2*$b*$r2)*($r2**($c/3))-($a*$r2**3 -
$b*$r2**2)*(($c/3)*$r2**(($c/3)-1)))/($r2**($c/3))**2");

ANS($ans2->cmp);
#ANS(num_cmp($ans));

ENDDOCUMENT();
