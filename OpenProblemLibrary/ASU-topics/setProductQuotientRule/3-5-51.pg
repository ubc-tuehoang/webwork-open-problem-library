DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context('Numeric');

$a = random(2, 4, 1);
$b = random(3, 8,1);
$c = random(2,7,1);
$d = random(3,9,1);
$f = random(2,6,1);
$g = random(4, 12, 1);
$r1 = random(1,5,1);
$r2 = random(-7,-1,1);
$h1 = $a*(($r1)**(.5));
$dh1 = ($a/2)*(($r1)**(-.5));
$l1 = $d*(($r1)**2) + $f*($r1) - $g;
while ($d*(($r1)**2) + $f*($r1) - $g == 0)
{
$g = random(4, 12, 1);
$l1 = $d*(($r1)**2) + $f*($r1) - $g;
}
$dl1 = 2*$d*$r1 + $f;

BEGIN_TEXT
Let
\[f(x) =  \frac{$a \sqrt{x} }{$d x^2 + $f x - $g}. \]
Evaluate \(f'(x)\) at \(x = $r1\).
$BR$BR
\( f'($r1) \) = \{ans_rule(50) \}
END_TEXT

$ans = Compute("(($l1)*($dh1) - ($h1)*($dl1))/(($l1)**2)");
ANS($ans->cmp);
#ANS(num_cmp($ans));


ENDDOCUMENT();
