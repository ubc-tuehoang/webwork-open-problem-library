## MLT(quotRulenoTrigRatFun)
## Level(2)
## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')
## MO(1)


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context('Numeric');

$a = random(2, 13, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$d = random(3,9,1);
$r1 = random(1,12,1);
while ($c*$r1 - $d == 0)
{
$r1 = random(1,12,1);
}
$r2 = random(-12,0,1);
while ($c*$r2 - $d == 0)
{
$r2 = random(1,12,1);
}

BEGIN_TEXT
Let
\[ f(x) = \frac{$a x^2 + $b x}{$c x - $d}. \]
Evaluate \( f'(x) \) at the following points:
$BR$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
END_TEXT

$ans = Compute("((2*$a*($r1) + $b)*($c*($r1) - $d) - ($a*(($r1)**2) + $b*($r1))*($c))/($c*$r1 - $d)**2");
ANS($ans->cmp);

BEGIN_TEXT
$BR (B) \( f'($r2) \) = \{ ans_rule(40) \}
$BR
END_TEXT

$ans2 = Compute("((2*$a*($r2) + $b)*($c*($r2) - $d) - ($a*(($r2)**2) + $b*($r2))*($c))/($c*$r2 - $d)**2");
ANS($ans2->cmp);

ENDDOCUMENT();
