  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
Context('Numeric');

$a = random(2,20,1);

BEGIN_TEXT

Suppose that \[ f(x) = \frac{x+$a}{ e^{x} }. \]  Find \(f'(2)\).

$BR $BR

\(f'(2)\) = \{ans_rule(30) \}

END_TEXT

$ans = Compute("(-1-$a)/exp(2)");
ANS($ans->cmp);


