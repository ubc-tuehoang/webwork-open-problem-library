DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);


$r1 = random(1,3,1);
$r2 = random(4,6,1);
$b = $r1+$r2;
$c = $r1*$r2;
$r1s = $r1**2;

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

TEXT(EV3(<<'EOT'));
Use \{helpLink("interval notation")\} to indicate where
\[ f(x) = \frac{x^2 - $r1s}{x^2 - $b x + $c}  \]
is continuous.
$PAR
\( f \) is continuous on
\{ans_rule(60)\}
EOT

Context("Interval");
$ans = Compute("(-Inf,$r1)U($r1,$r2)U($r2,Inf)");
ANS($ans->cmp);

ENDDOCUMENT();
