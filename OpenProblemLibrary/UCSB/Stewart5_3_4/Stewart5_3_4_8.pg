$c=Real(random(1,10,1)*random(-1,1,2));

Context()->texStrings;
BEGIN_TEXT
Differentiate \(y =e^u (\cos{u}+$c u)\).
 
$BR $BR

\(y' =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;

#ANS(fun_cmp("exp(u)*(cos(u)-sin(u)+c*u+c)", var=>["u","c"]));
$f = Compute("exp(u)*(cos(u)-sin(u)+$c*u+$c)");
ANS($f->cmp);

