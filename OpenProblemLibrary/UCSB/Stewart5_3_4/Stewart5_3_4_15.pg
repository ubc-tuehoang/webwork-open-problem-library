DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

Context("Numeric");

$f=Compute("sec(x)*tan(x)");
$df=Compute("sec(x)*tan^2(x)+sec^3(x)");

Context()->texStrings;

BEGIN_TEXT
Differentiate \(y =$f\). 
$BR $BR
\(y' =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;

ANS($df->cmp);

ENDDOCUMENT();
