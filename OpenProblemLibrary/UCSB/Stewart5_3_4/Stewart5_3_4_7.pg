DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

Context("Numeric");
Context()->variables->are(t=>"Real");

$a=Real(random(1,10,1)*random(-1,1,2));

Context()->texStrings;
BEGIN_TEXT
Differentiate \(h(t) =$a \csc{t} + e^t \cot{t}\). 

$BR $BR

\(h'(t) =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;

$f = Compute("-$a*csc(t)*cot(t)+exp(t)*(-(csc(t))^2+cot(t))");
ANS($f->cmp);

ENDDOCUMENT();
