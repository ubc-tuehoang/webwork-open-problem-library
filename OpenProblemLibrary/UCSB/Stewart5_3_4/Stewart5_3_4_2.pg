DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

Context("Numeric");

$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$f = Compute("$a*x*sin(x)");
$df=$f->D('x');

Context()->texStrings;

BEGIN_TEXT
Differentiate \(f(x)=$f\). 

$BR $BR

\(f'(x) =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;

ANS($df->cmp);

ENDDOCUMENT();
