DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

Context("Numeric");

Context()->texStrings;

BEGIN_TEXT
If \(f(x)=\sqrt{x}\sin{x}\), find \(f'(x)\).

$BR $BR

\(f'(x) =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;

$df = Compute("x^(1/2)*cos(x)+(1/2)*x^(-1/2)*sin(x)");
$df->{limits} = [1,3];
ANS($df->cmp);

ENDDOCUMENT();
