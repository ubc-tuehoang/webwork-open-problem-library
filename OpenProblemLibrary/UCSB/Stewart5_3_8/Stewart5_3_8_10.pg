DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
Context('Numeric');
Context()->variables->are('t'=>"Real");

$a=random(1,10,1);

BEGIN_TEXT


Differentiate \(\displaystyle f(t)=\frac{$a+\ln{t}}{$a-\ln{t}}\).

$BR $BR

\(f'(t) =\) \{ans_rule(30)\}

END_TEXT

$ans = Compute("2*$a/(t*($a-ln(t))^2) ");
ANS($ans->cmp);

ENDDOCUMENT();
