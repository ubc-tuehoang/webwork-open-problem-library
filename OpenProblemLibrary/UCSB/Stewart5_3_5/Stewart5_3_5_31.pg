#####################################################################
DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
#####################################################################

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

#####################################################################

Context("Numeric");
$a = random(2,10,1);
$ans = Compute("(ln($a))(pi)($a)^(sin ((pi)*x))cos((pi)*x)");

#####################################################################
Context()->texStrings;
BEGIN_TEXT

Differentiate \( y = {$a}^{\sin{\pi x}}\).

$BR$BR

\(y'=\) \{ans_rule(60)\}

END_TEXT
Context()->normalStrings;

#####################################################################

ANS($ans -> cmp);

ENDDOCUMENT();
