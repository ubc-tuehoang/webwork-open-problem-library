DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are(u => 'Real');

$a=random(2,9,1)*random(-1,1,2);

BEGIN_TEXT
Differentiate:
\[
Y(u)= (u^{-2}+u^{-3})(u^5+$a u^2)
\]
$BR$BR

\(Y'(u) =\) \{ans_rule(50)\}

END_TEXT

$answer = Compute("(-2u^{-3}-3u^{-4})(u^5+$a u^2)+(u^(-2)+u^(-3))(5u^4+2*($a)u)");

ANS($answer->cmp);

ENDDOCUMENT();

