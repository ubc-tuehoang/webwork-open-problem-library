## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Antiderivatives (without trigonometric functions))
## Institution(UCSB)
## MLT(antideriv_simplify_rational_expressions)
## MLTleader(1)
## MO(1)
## Level(3)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('4.10')
## Problem1('11')
## KEYWORDS('antiderivatives')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

Context("Numeric"); 
Context()->variables->are(u => "Real", limits => [.1,3]);

Context()->texStrings;
BEGIN_TEXT

$PAR
Find the most general antiderivative of \(\displaystyle f(u) = \frac{$a u^4 + $b \sqrt{u}}{u^2}.\) 

$PAR
Note: Any arbitrary constants used must be an upper-case "C".

$PAR
\(F(u) =\) \{ans_rule(50)\}

END_TEXT

Context()->normalStrings;
$answer = FormulaUpToConstant("$a*u^(3)/3+$b*u^(-1/2)/(-1/2)+C");

ANS($answer->cmp()); 

ENDDOCUMENT();
