DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());


TEXT(EV2(<<EOT)); 
If
\[
A = \begin{bmatrix}1 & 1 \\ -1 & 2 \end{bmatrix}
\]
then

\[
A^{-1} =
\begin{bmatrix}a & b \\ c & d \end{bmatrix}
\]
where
$BR
\(a = \) \{ ans_rule(10) \} 
EOT
ANS(num_cmp(2/3));

TEXT(EV2(<<EOT)); 
$BR
\(b = \) \{ ans_rule(10) \}
EOT
ANS(num_cmp(-1/3));

TEXT(EV2(<<EOT)); 
$BR
\(c = \) \{ ans_rule(10) \} 
EOT
ANS(num_cmp(1/3));

TEXT(EV2(<<EOT)); 
$BR
\(d = \) \{ ans_rule(10) \} 
EOT
ANS(num_cmp(1/3));

TEXT(EV2(<<EOT)); 
$BR
Use it to solve the linear system
\[
\begin{array}{rcl}
x+y &=& 1 \\
-x+2y &=& 2 \\
\end{array}
\]

\(x = \) \{ ans_rule(10) \} 
EOT
ANS(num_cmp(0));

TEXT(EV2(<<EOT)); 
$BR
\(y = \) \{ ans_rule(10) \} 
EOT
ANS(num_cmp(1));

ENDDOCUMENT();        # This should be the last executable line in the problem.
