## updated 12/2014

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
Context('Numeric');

$an = random(2,7,1);
$as = random(-1,1,2);
$a  = $an * $as;

$bn = random(2,8,1);
$bs = random(-1,1,2);
$b  = $bn * $bs;

$cn = random(2,7,1);
$cs = random(-1,1,2);
$c  = $cn * $cs;

$funct1 = "$a - $c/x^2";

BEGIN_TEXT
If \(\displaystyle f(x) =  \frac {$a x^5 + $b x^4 + $c x^3} {x^4}\) find \( f'( x ) \).
$BR $BR \( f'( x ) \) = \{ans_rule(30) \}
$BR
END_TEXT

$ans = Compute("$funct1");
ANS($ans->cmp);




ENDDOCUMENT();        # This should be the last executable line in the problem.
