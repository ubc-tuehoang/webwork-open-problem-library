DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
Context('Numeric');

$an = random(2,5,1);
$as = random(-1,1,2);
$a  = $an * $as;
$b  = random(2,8,1);
$x  = random(2,5,1);

$ans1 = Formula("$a/x")->reduce;
$ans2 = $ans1->substitute(x=>$x);

Context()->texStrings;
BEGIN_TEXT
Let \[ f(x) = $a \ln($b x) \]
$BR$BR
\( f'( x ) = \) \{ans_rule(40) \}
$BR$BR
\( f'( $x ) = \) \{ans_rule(40) \}
END_TEXT
Context()->normalStrings;

ANS($ans1->cmp());
ANS($ans2->cmp());

ENDDOCUMENT();        # This should be the last executable line in the problem.
