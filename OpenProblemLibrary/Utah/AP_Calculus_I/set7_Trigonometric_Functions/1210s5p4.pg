DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

Context('Numeric');

BEGIN_TEXT
Let
\[f(x) = \sin\frac{1}{x}.\]
$BR$BR
\(f'(x) = \) \{&ans_rule(45)\}.
END_TEXT
$ans="-cos(1/x)/(x*x)";
ANS(fun_cmp($ans, limits=>[[0.1,1]], vars=>["x"]));

BEGIN_TEXT
$BR$BR
Let
\[g(x) = \frac{1}{\sin x}.\]
$BR
\(g'(x) = \) \{&ans_rule(45)\}.
END_TEXT
$ans="-cos(x)/sin(x)**2";
ANS(fun_cmp($ans, limits=>[[0.1,1]], vars=>["x"]));






