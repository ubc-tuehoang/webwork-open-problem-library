DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
Context("Numeric");

$a=random(2,5);
do {$b = random(1,10)} until ($b != $a);
$a=Real($a);
$b=Real($b);
$f=Compute("sin(x)*(x^($a)-$b)");
$df=$f->D('x');

Context()->texStrings;

BEGIN_TEXT
Compute \(D_x \big( $f \big)\).
$BR $BR
\(D_x \big( $f \big)=\)\{ans_rule(45)\}
END_TEXT

Context()->normalStrings;

ANS($df->cmp);

ENDDOCUMENT();
