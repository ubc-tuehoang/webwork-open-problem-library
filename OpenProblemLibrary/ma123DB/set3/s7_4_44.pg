DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);


$showPartialCorrectAnswers = 1;
$a = random(3,7,1);
$b = random(4,8,1);
if ($a == $b) {$b++;}

TEXT(beginproblem());


BEGIN_TEXT
Evaluate the indefinite integral. $BR
\[ \int \frac{1}{$a x +$b \sqrt{x}}\, dx\]
$BR
$BR
Answer: \{ ans_rule(40)\} \(+\) \(C\)
END_TEXT

ANS(fun_cmp("2*ln($a*sqrt(x)+$b)/$a",mode=>'antider'));
ENDDOCUMENT();




