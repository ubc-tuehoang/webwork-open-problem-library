);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$funct = "cos(x)^5/5-cos(x)^3/3";

BEGIN_TEXT

Evaluate the indefinite integral.
$BR \[ \int \sin^3 x \cos^2 x \,dx \]
$BR
Answer: \{ans_rule( 45) \} \(+\) \(C\)
$BR
END_TEXT

ANS(fun_cmp($funct, mode=>'antider'));

