DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1;

$aa = random(3,8,1);
$bb = random(5,10,1);
if ($aa == $bb) {$aa++;}
$a2 = $aa**2;
$b2 = $bb**2;

TEXT(beginproblem());

BEGIN_TEXT
$BR
\( \displaystyle \int \frac{1}{\sqrt{$a2 - $b2 x^2}}dx\) =   \{ ans_rule(45)\} +\(C\)

END_TEXT

ANS(fun_cmp("arcsin($bb*x/$aa)/$bb",mode=>'antider'));

ENDDOCUMENT();
