########################################################################

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#

Context("Numeric");

$a = random(2,8,1);
$b = random(2,8,1);
$c = random(2,4,1);

$f = Formula("$a*arctan($b*sin($c*x))");
$dfdx = $f->D;

##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT

If \( f(x) = $f \), find \( f'( x ) \). $PAR
\{ans_rule(50) \} $BR

END_TEXT
Context()->normalStrings;

##############################################################
#
#  Answers
#
#

ANS($dfdx->cmp);

$bb=$b*$b;
$abc=$a*$b*$c;
BEGIN_SOLUTION
$PAR SOLUTION $PAR
\[ \begin{array}{rcl} f'(x) &=& \displaystyle\frac{$a}{1+ ($b \sin($c x))^2} \frac{d}{dx} ($b \sin($c x))
\\ &=& \displaystyle\frac{$a}{1+ $bb \sin^2($c x)} ($b\cdot $c  \cos ($c x))
\\ &=& \displaystyle \frac{$abc \cos ($c x)}{1+ $bb \sin^2($c x)} 
\end{array}
\]
END_SOLUTION
;

;
ENDDOCUMENT();
