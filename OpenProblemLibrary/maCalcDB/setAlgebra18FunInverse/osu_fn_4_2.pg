##Find formula for inverse function for a function involving
##exponentials
##Ellis & Gulick section 6.2
##Authored by Zig Fiedorowicz 1/29/2000
##ENDDESCRIPTION

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);


$showPartialCorrectAnswers = 1;

$AA = random(1,6,1);
$BB = random(3,12,1);
$CC = random(7,25,1);
$DD = random(6,15,1);

$answ1 = -$BB/$DD;
$answ2 = $AA/$CC;
TEXT(beginproblem());
BEGIN_TEXT
\[f(x)=\frac{$AA e^x - $BB}{$CC e^x + $DD}\]
$BR
\(f^{-1}(x)\)  =  \{ ans_rule()\}
$BR
The domain of \(f^{-1}(x)\) is the open interval \((a,b)\), where $BR
\(a\)  =  \{ ans_rule()\}
$BR
and
\(b\)  =  \{ ans_rule()\}
END_TEXT

ANS(fun_cmp("ln(($DD*x+$BB)/($AA-$CC*x))", limits=>[$answ1+.0001,$answ2-.0001], vars=>"x"));
ANS(num_cmp($answ1));
ANS(num_cmp($answ2));

ENDDOCUMENT();
