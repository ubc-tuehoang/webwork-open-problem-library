## KEYWORDS('logarithms', 'exponentials')

##DIFFICULTY('easy')
##  Evaluate simple expressions involving logs and exponentials
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
$a1 = random(-11,11,2);
$b1 = random(2,7,1);
$d1 = random(2,4,1);
$i1 = random(2,5,1);

BEGIN_TEXT;
Evaluate the following expressions. Your answers must be exact and in simplest form.
$PAR
(a) \( \ln e ^ { $a1 } = \) \{ans_rule(10) \}
END_TEXT

$ans = $a1;
ANS(num_cmp($ans, mode=>"strict"));

BEGIN_TEXT;
$PAR
(b) \( e ^ {\ln $b1 } = \) \{ans_rule(10) \}
END_TEXT

$ans = $b1;
ANS(num_cmp($ans, mode=>"strict"));

BEGIN_TEXT;
$PAR
(c) \(e ^ {\ln \sqrt { $d1 }  } = \) \{ans_rule(10) \}
END_TEXT

$ans = sqrt($d1);
ANS(num_cmp($ans));

BEGIN_TEXT;
$PAR
(d) \( \ln ( 1/e^ { $i1} ) = \) \{ans_rule(10) \}
END_TEXT

$ans = -$i1;
ANS(num_cmp($ans, mode=>"strict"));

ENDDOCUMENT();        # This should be the last executable line in the problem.;
