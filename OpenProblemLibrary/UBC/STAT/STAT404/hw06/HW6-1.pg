$showPartialCorrectAnswers = 1;
ANS( num_cmp($ansa, tol=> 0.005) );
ANS( num_cmp($ansb, tol=> 0.005) );
ANS( num_cmp($ansc, tol=> 0.005) );
ANS( num_cmp($ansd, tol=> 0.005) );

