$graph4 = init_graph(-1,-1,13,9,'axes'=>[0,0],'ticks'=>[14,10],'grid'=>[14,10],'size'=>[560,400]);
$graph4->new_color("lightblue", 214,230,244);
$p1 = FEQ("$f1 for x in [0,6> using color:blue weight:2");
