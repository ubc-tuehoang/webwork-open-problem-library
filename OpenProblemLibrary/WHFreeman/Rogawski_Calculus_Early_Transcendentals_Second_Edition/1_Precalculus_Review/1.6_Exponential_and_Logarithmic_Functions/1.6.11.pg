DOCUMENT();

#Load Necessary Macros

loadMacros(
  "PGstandard.pl",
  "Parser.pl",
  "freemanMacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

#Book Values
# $base = 3;
# $constant = 27;

#Randomized Values

$base = random(3, 5);
$solution = random(2, 4);
$sol = Real("$solution");
$constant = $base**$solution;

Context()->texStrings;


BEGIN_TEXT
\{ textbook_ref_exact("Rogawski ET 2e", "1.6", "11") \}

$PAR
Calculate directly (without using a calculator.)
$PAR


\( \log_{$base} $constant \)
$PAR

Solution: \{ans_rule() \}
$PAR
END_TEXT

Context()->normalStrings;

#Answer Checker
ANS($sol->cmp);

Context()->texStrings;

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR
$SOL
$BR
\(\log_{$base} $constant =\log_{$base} $base^{$solution} = 
$sol \)
$BR

END_SOLUTION

ENDDOCUMENT();
