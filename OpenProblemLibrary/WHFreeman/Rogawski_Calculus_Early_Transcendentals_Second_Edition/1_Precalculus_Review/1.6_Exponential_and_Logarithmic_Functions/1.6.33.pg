DOCUMENT();

#Load Necessary Macros

loadMacros(
  "PGstandard.pl",
  "Parser.pl",
  "freemanMacros.pl",
  "PGcourse.pl"
);


#Book Values
# $exponent1 = 4;
# $exponent2 = 2;
# $constant = 2;

TEXT(beginproblem());

#Randomized Values

$exponent1 = random(6, 10);
$exponent2 = random(2, 5);
$constant = $exponent1 - $exponent2;

Context()->texStrings;


BEGIN_TEXT
\{ textbook_ref_exact("Rogawski ET 2e", "1.6", "33") \}

$PAR
Solve for the unknown variable.
$PAR


\( \ln (x^{$exponent1}) - \ln (x^{$exponent2}) = $constant \)
$PAR

\( x = \)  \{ans_rule() \}
$PAR
END_TEXT

Context()->normalStrings;

#Answer Checker
$solution = Real("e");
ANS($solution->cmp);

Context()->texStrings;

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR
$SOL
$BR
\( \ln(x^{$exponent1})-\ln(x^{$exponent2})=\ln ( \frac{x^{$exponent1}}{x^{$exponent2}} )=\ln (x^{$constant})= $constant \ln x \). Thus, \( $constant \ln x =$constant \) or \( \ln x =1 \).  
$PAR Hence, \( x=e \).
$BR

END_SOLUTION

ENDDOCUMENT();
