######################################
## Answers to the questions
######################################
Context("Numeric");

$L3 = Formula("5");
$R3 = Formula("14");
$M3 = Formula("8.75");
$T3 = Formula("9.5");

$I = Formula("9"); 

$errorL3 = $I-$L3;
$errorR3 = $I-$R3;
$errorM3 = $I-$M3;
$errorT3 = $I-$T3;

Context()->variables->add(b => 'Real');
Context()->variables->add(B => 'Real');
Context()->variables->add(h => 'Real');
$areaTrap = Formula(".5*(b+B)*h");


TEXT(beginproblem());


