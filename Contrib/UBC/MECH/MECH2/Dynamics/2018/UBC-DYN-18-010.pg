DOCUMENT();

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
	#"source.pl",        # allows code to be displayed on certain sites.
	#"PGcourse.pl", 	 # Customization file for the course
   	);

TEXT(beginproblem());

##############################################################
#
#  Setup
#
#

Context("Numeric");
#given
$OB = 0.9; #m
$BD = 0.6; #m
$AB = 1.2; #m
$VA = random(.2,.5,.1);


#image scale
$imgScale = .2;

#image aspect ratio
$width = $imgScale*1853;
$height = $imgScale*1845;

#computation
$AC = 2; #m
$DC = 1.709; #m
$Beta = 32.6; #degree
$VD = ($DC/$AC)*$VA;
$VE = $VD*cos($Beta*pi/180);



#set tolerance
Context()->flags->set(
tolerance=>.02);

##############################################################
#
#  PGML 
#
#

BEGIN_PGML

In the figure shown below, find the downward velocity of the plunger at the time where [`\theta = 90^{\circ}`] and the velocity of the roller [`A`] is [`[$VA]`] [`m/s`]. 

[@ image( "UBC-DYN-18-010.png", width=>[$width], height=>[$height]) @]*

[`V_E=`][____]{"$VE"} [`m/s`]

END_PGML

