DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserRadioButtons.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
TEXT(beginproblem());
Context("Matrix");

$A = Matrix([[(2,3)],[4,1]]);

$B = Matrix([[-2/5,1/5],[8/5,-4/5]]);
$C = Matrix([[1/2,1/3],[1/4,1]]);
$D = $A->inverse;
$E = Matrix([[1/4,-2/4],[-2/4,9/4]]);

Context()->texStrings;

$mc = RadioButtons(
    [ "\( $B \)", "\( $C \)", "\( $D \)", "\( $E \)", "None of the above"]
    , "\( $D \)"
    ,last => [ "None of the above"]
    ,labels => "ABC" );

BEGIN_TEXT

Consider the matrix \(A = $A\). What is \(A^{-1}\) (the inverse of \(A\))?
$PAR
\{ $mc->buttons() \}

END_TEXT

$showPartialCorrectAnswers = 0;

ANS( $mc->cmp() );

ENDDOCUMENT();
